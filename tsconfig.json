{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowJs": true,
    "jsx": "react",
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "dist"],
  "paths": {
    "@env": ["env.d.ts"]
  },
  "extends": "expo/tsconfig.base",
  "include": ["src", "env.d.ts"]
}
